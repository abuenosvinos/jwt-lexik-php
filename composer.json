{
    "name": "antonio/jwt-lexik-php",
    "description": "Implementation of JWT using https://github.com/lexik/LexikJWTAuthenticationBundle",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abuenosvinos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abuenosvinos\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Antonio Buenosvinos",
            "email": "antonio.buenosvinos@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/flex": "^2.3",
        "lexik/jwt-authentication-bundle": "^2.19",
        "symfony/framework-bundle": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/console": "^6.3",
        "symfony/runtime": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2.1",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/phpunit-bridge": "^6.3"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
